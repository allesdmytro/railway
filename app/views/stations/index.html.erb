<h1>Stations#index</h1>
<p>Find me in app/views/stations/index.html.erb</p>
<% if current_user && current_user.admin? %>
  <%= link_to 'Create new station', new_station_path, class: "button is-primary" %>
<% end %>
<% @stations.each do |station| %>
  <br>
  <%= link_to station.title, station %>
  <br>
<% end %>